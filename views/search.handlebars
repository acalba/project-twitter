<script src="/scripts/game.js"></script>
<script src="/scripts/jquery.lettering.js"></script>
<div id="countdown">
  <h1></h1>
</div>

<div id="game">

  <h2 class="page-header">Round 1</h2>
  <div class="search-tweets col-sm-8">
    <ul class="tweets-ul">
    {{#each tweets}}
       <li>
        <img src="/images/twitter_logo.png" alt="" width="200px" height="40px">
        <blockquote class="twitter">
          <p class="twitter-text">{{text}}<p>
        </blockquote>
      </li>
    {{/each}}
    </ul>
  </div>
  <input type="text" name"typingArea" id="typingArea" autofocus="autofocus" size="130" class="form-control">
</div>